domain = dhs_growth@data$ext.growthrate
,bins=c(-.4,-0.096711, -0.076584, -0.049980 ,-0.026858,-0.003892,  0.022136,  0.070742,3.8)
)
leaflet(data=dhs_growth) %>%
setView(0, 0, zoom=2) %>%
addPolygons(color = ~pal(ext.growthrate)
,fillOpacity = 1
,stroke=F
,smoothFactor=0.2
,popup=paste(dhs_growth@data$CNTRYNAMEE,":",dhs_growth@data$DHSREGEN,round(dhs_growth@data$ext.growthrate,2))) %>%
addLegend("bottomright", pal=pal, values = ~ext.growthrate, opacity = 1)
?percentile
?quantile
list.of.packages <- c("sp","rgdal","leaflet","data.table")
new.packages <- list.of.packages[!(list.of.packages %in% installed.packages()[,"Package"])]
if(length(new.packages)) install.packages(new.packages)
lapply(list.of.packages, require, character.only=T)
if(.Platform$OS.type == "unix"){
prefix = "~"
}else{
prefix = "E:"
}
wd = paste0(prefix,"/git/p20_spatial_2018")
setwd(wd)
growthrates=read.csv("project_data/regionswide20180720.csv",as.is=T)
load("project_data/recent_dhs_1.RData")
dhs_growth=merge(recent_dhs_1,growthrates,by=c("OBJECTID","DHSCC"))
dhs_growth=subset(dhs_growth,!is.na(NP20HC.0))
dhs_growth@data$value=dhs_growth@data$ext.growthrate*100
pal <- colorBin(
palette = "YlOrRd",
domain = dhs_growth@data$value
,bins=quantile(dhs_growth@data$value,probs=seq(0,1,0.2))
)
leaflet(data=dhs_growth) %>%
setView(0, 0, zoom=2) %>%
addPolygons(color = ~pal(value)
,fillOpacity = 1
,stroke=F
,smoothFactor=0.2
,popup=paste0(
"<b>",dhs_growth@data$CNTRYNAMEE,"</b> <br/>",
"<b>Region Name: </b>",dhs_growth@data$DHSREGEN,"<br/>",
"<b>Value: </b>",round(dhs_growth@data$value,2))
) %>%
addLegend("bottomright", pal=pal, values = ~value, opacity = 1)
quantile(dhs_growth@data$value,probs=seq(0,1,0.2))
